        <script>
            function mapDisplay($lat,$lon) {
                var mapOptions = {
                    zoom: 16,
                    center: new google.maps.LatLng($lat,$lon),
                    mapTypeId: google.maps.MapTypeId.ROADMAP
                };
                var map = new google.maps.Map(document.getElementById('gmap'), mapOptions);
            };

            jQuery(function() {
                var $lat = 40.768878;
                var $lon = -73.836155;
                mapDisplay($lat,$lon);

                jQuery('.showmap').click(function(e){
                    e.preventDefault();
                    var $this = jQuery(this);
                    var $lat1 = $this.attr('data-lat');
                    var $lon1 = $this.attr('data-lon');
                    mapDisplay($lat1,$lon1);

                    jQuery('#map-detail').html($this.parent('.col-md-4').find('.map-detail'));
                    jQuery("html,body").clearQueue().animate({
                        scrollTop:jQuery('#main').offset().top
                    },1800,"swing");
                });
            });
        </script>
