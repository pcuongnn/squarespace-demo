<section id="content">
  <div class="bg-header"><img src="../../../../assets/images/bg-header.png" alt=""></div>
<div class="container">
{.section item}

  <!--WRAPPER-->
  <a href="http://smilist-demo.squarespace.com/blog/?format=rss" title="SUBSCRIBE" class="social-rss" style="
margin-left: 90%;font-weight: bold;">&nbsp;SUBSCRIBE</a> 
  <article id="post-{id}" class="{@|item-classes}" data-item-id="{id}">
    
    <!--POST TILE-->

    <h1 class="title" data-content-field="title">
      {.passthrough?}
        <a href="{sourceUrl}" target="_blank">{title}</a>
      {.or}
        <a href="{fullUrl}">{title}</a>
      {.end}
    </h1>

    <!--MAIN CONTENT-->

    {body}

    <!--BLOG INJECTION-->

    {postItemInjectCode}    

    <!--CATEGORIES-->

    {.repeated section categories}
      <a class="category" href="{collection.fullUrl}?category={@|url-encode}">{@}</a>{.alternates with},
    {.end}

    <!--TAGS-->

    <br>
    Tags:
    <br>
    {.repeated section tags}
      <a class="tag" href="?tag={@|url-encode}">{@}</a>{.alternates with}&nbsp;
    {.end}

    <!--SHARE AND LIKE-->

    {@|social-button}

    <!--AUTHOR AND DATE-->
    
    <p class="meta">
      Published on <time datetime="{addedOn|date %F}">{addedOn|date %B %d, %Y}</time> by <a href="{collection.fullUrl}?author={author.id}">{author.displayName}</a>.
      {.section sourceUrl}
        <a href="{@}" class="source" target="_blank">Source</a>.
      {.end}
    </p>        

    <!--LOCATION-->

    {.section location}
      {.section addressTitle}
        {addressTitle}
        {addressLine1}
        {addressLine2}
        {addressCountry}
      {.end}
    {.end}

    <!--COMMENTS-->

    {@|comments}

  </article>

{.end}
    
<div class="container">
</section>